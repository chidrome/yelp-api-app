<h5>More info on the restaurant</h5>

<div class="row">
    <div class="col s6">
        <img class="profileImg" src="<%= restaurant.image_url %>" alt="<%= restaurant.name %>">
        <h5><%= restaurant.name %></h5>
        <p class="address addressTitle">Addres</p>
        <p class="address"><%= restaurant.address1 %></p>
        <p class="address"><%= restaurant.address2 %></p>
        <p class="address"><%= restaurant.address3 %></p>
        <p class="address"><%= restaurant.city %>, <%= restaurant.state %></p>
        <p class="address"><%= restaurant.zip %></p>
        <p>Phone: <%= restaurant.phone %></p>
    </div>
    <div class="col s4">
        <% include ../moreinfo-map.ejs %> 
    </div>
</div>


